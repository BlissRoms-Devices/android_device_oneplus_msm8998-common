<resources>

    <!-- IDs for each color mode. The values must match the corresponding constants in
         android.view.Display -->
    <integer-array name="color_mode_ids">
        <item>0</item>
        <item>-1</item>
        <item>-1</item>
    </integer-array>
    
    <!-- Override supported color mode labels.
         NOTE: This list must have the exact same items count as config_availableColorModes -->
    <string-array name="available_vendor_color_modes" translatable="false">
        <item>Vivid</item>
        <item>Natural</item>
        <item>AMOLED Wide Gamut</item>
        <item>sRGB</item>
        <item>Display P3</item>
    </string-array>

</resources>
